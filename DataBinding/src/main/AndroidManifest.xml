<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.databinding">

  <application
      android:name=".App"
      android:allowBackup="true"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme">
    <activity android:name=".MainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN"/>

        <category android:name="android.intent.category.LAUNCHER"/>
      </intent-filter>
    </activity>
    <activity
        android:name=".AdapterBindingActivity"
        android:label="RecycleView适配器绑定"/>
    <activity
        android:name=".DataBindingOneWayActivity"
        android:label="单向数据绑定">
    </activity>
    <activity
        android:name=".DataBindingTwoWayActivity"
        android:label="双向数据绑定">
    </activity>
    <activity
        android:name=".CustomBindingAdapterActivity"
        android:label="自定义绑定">
    </activity>
    <activity android:name=".ObservableFiledActivity">
    </activity>
  </application>

</manifest>